{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "packageManager": "bun@1.2.2",
  "scripts": {
    "dev": "turbo run dev:build dev:serve dev:css",
    "dev:build": "bun-react-ssg build --watch",
    "dev:serve": "bun-react-ssg serve",
    "dev:css": "tailwindcss -i ./src/style.css -o ./dist/style.css --watch",
    "build": "bun-react-ssg build --url https://bun-react-ssg.pages.dev && bun run build:css",
    "build:css": "tailwindcss -i ./src/style.css -o ./dist/style.css"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.13",
    "bun-react-ssg": "workspace:*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.1.13"
  },
  "devDependencies": {
    "turbo": "^2.5.8",
    "bun-types": "^1.2.21"
  }
}
